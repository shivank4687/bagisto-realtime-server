# Server
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Laravel API
LARAVEL_API_URL=http://localhost:8000
LARAVEL_API_TIMEOUT=5000

# Redis (Optional - set REDIS_ENABLED=false to disable)
REDIS_ENABLED=false
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Socket.IO
SOCKET_CORS_ORIGIN=http://localhost:8000,http://127.0.0.1:8000
SOCKET_PING_TIMEOUT=60000
SOCKET_PING_INTERVAL=25000

# Logging
LOG_LEVEL=info
LOG_DIR=./logs

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
